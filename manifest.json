{
  "manifest_version": 2,
  "name": "MeBun Replacer",
  "version": "1.0",
  "description": "Replaces Facebook stickers with custom stickers in Facebook chats.",
  "icons": {
    "16": "Images/logo16.png",
    "48": "Images/logo48.png",
    "128": "Images/logo128.png" },

  "browser_action": {
    "default_icon": "Images/logo16.png"
  },

  "content_scripts":
  [
    {
      "matches": ["*://*/*"],
      "js": ["myscript.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources":
  [
    "images/*"
  ]
}
